int(input("введите значение Y:"))
def determine_quardant (x,y):
    if x > 0 and y > 0:
        print ( " точка находится в первой четверти")
    elif x < 0 and y > 0:
        print ( " точна находится во второй четверти")
    elif x < 0 and y < 0: 
        print ( " точка находится в третьей четверти")
    elif x > 0 and y < 0:
        print ( " точка находится в четвертой четверти")
    elif x == 0 and y != 0:
        print (" точка лежит на осви ординат")
    elif y == 0 and x != 0:
        print ( " точка лежит на осви абсцисс")
    else:
        print (" точка находится в начале координат")
